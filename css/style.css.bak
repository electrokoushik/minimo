/*Common start*/
body{
	margin: 0;
	padding: 0;
	font-family: 'Playfair Display', serif;
	font-size: 16px;
	font-weight: 400;
	color: #626262;
	line-height: 24px;
}

.container{
	margin: 0 auto;
	width: 90%;
	overflow: hidden;
}

.wrapper{
	margin: 0 auto;
	width: 80%;
}
/*Common end*/

/*Header start*/
header{
	padding: 25px 0 35px;
}
.container{
	margin: 0 auto;
	width: 90%;
	overflow: hidden;
}
.logo{
	margin-top: 14px;
	width: 11%;
	float: left;
}
.logo img{
	width: 100%;
}
.menu{
	width: 70%;
	float: right;
}
.menu ul{
	float: right;
}
.menu ul li{
	list-style: none;
	float: left;
}
.menu ul li a{
	margin: 0 60px;
	padding: 1px 2px;
	display: inline-block;
	color: #626262;
	text-decoration: none;
	text-transform: uppercase;
	border-top: 1px solid transparent;
	border-bottom: 1px solid transparent;
	transition: .3s ease-in-out;
}
.menu ul li:last-child a{
	margin-right: 0;
}
.menu ul li a:hover{
	border-top: 1px solid #626262;
	border-bottom: 1px solid #626262;
}
/*Header end*/

/*Banner start*/
.b_content img{
	width: 100%;
}
/*Banner end*/

/*Article start*/
.article{
	padding: 20px 0 10px;
}

.c_tag{
	margin: 0;
	font-family: 'Ubuntu', sans-serif;
	font-size: 14px;
	font-weight: 300;
	color: #b4ad9e;
	text-transform: uppercase;
}
.c_title{
	margin: 15px 0 25px;
	font-size: 30px;
	font-weight: 400;
	line-height: 40px;
}
.c_text{
	margin: 0;
}
.c_paragraph{
	margin-bottom: 40px;
}
/*Article end*/

/*Blog start*/
.blog{
	padding: 20px 0 120px;
}
.blog_content{
	overflow: hidden;
}
.single_blog{
	margin-top: 60px;
	width: 47%;
	float: left;
}
.single_blog:nth-child(even){
	margin-left: 6%;
}
.blog_image{
	margin-bottom: 15px;
}
.blog_image img{
	width: 100%;
}
/*Blog Section end*/

/*Newsletter Section start*/
.news{
	background-color: #f0f0f0;
}
.news_box{
	margin: 0 auto;
	padding: 70px 0;
	width: 60%;
	text-align: center;
}
.signup{
	margin-top: 0;
	margin-bottom: 50px;
	color: #333333;
}
.signup_form{
	position: relative;
	margin: 0 auto;
	width: 400px;
}
.signup_form input{
	padding-right: 55px;
	width: 300px;
	height: 45px;
	font-family: 'Playfair Display', serif;
    font-size: 14px;
    color: #626262;
    background-color: transparent;
    border: none;
    border-bottom: 1px solid #000000;
}
.signup_form input:focus{
	outline: 0;
}
.signup_form button{
	position: absolute;
	top: 15px;
	right: 18px;
	background-color: transparent;
	border: none;
	cursor: pointer;
}
.signup_form button:focus{
	outline: 0;
}
.signup_form button img{}
/*Newsletter end*/

/*Footer start*/
footer{
	padding: 15px 0 20px;
	background-color: #f0f0f0;
}
.f_left{
	width: 50%;
	float: left;
}
.f_left ul{
	padding: 0;
	float: left;
}
.f_left ul li{
	float: left;
	list-style: none;
}
.f_left ul li a{
	margin: 10px 10px;
	color: #000000;
	text-decoration: none;
	border-top: 1px solid transparent;
	border-bottom: 1px solid transparent;
	transition: .33s ease-in-out;
}
.f_left ul li a:first-child{
	margin-left: 0;
}
.f_left ul li a:hover{
	border-top: 1px solid #626262;
	border-bottom: 1px solid #626262;
}
.f_right{
	width: 50%;
	float: right;
}
.f_right span{
	color: #000000;
}
.f_right ul{
	padding: 0;
	float: right;
}
.f_right ul li{
	float: left;
	list-style: none;
}
.f_right ul li a{
	padding: 5px 20px;
}
.f_right ul li a:last-child{
	padding-right: 0;
}
.developer{
	text-align: center;
}
.developer a{
	margin: 0;
	padding: 0;
	font-family: 'Playfair Display', serif;
	font-size: 16px;
	font-weight: 600;
	color: #626262;
	text-decoration: none;
	transition: .33s ease-in-out;
}
.developer a:hover{
	color: #000000;
}
/*Footer end*/